\documentclass{article}
\usepackage{algorithm}
\usepackage{algpseudocode} % Zamiana algorithmic na algpseudocode
\usepackage[polish]{babel}
\usepackage[utf8]{inputenc}
\usepackage[T1]{fontenc} % Dodanie kodowania T1 dla polskich znaków

\begin{document}

\begin{algorithm}
\caption{Mój algorytm}
\begin{algorithmic}[1]
\Require $n \geq 0$
\Ensure $a = x^n$
\State $k \gets n$; $a \gets 1$; $b \gets x$
\While{$k > 0$} \Comment{Niezmiennik: $x^n = a \cdot b^k$}
    \If{$k$ jest liczbą parzystą}
        \State $k \gets k / 2$
        \State $b \gets b \cdot b$
    \Else 
        \State $k \gets k - 1$
        \State $a \gets a \cdot b$
    \EndIf
\EndWhile
\end{algorithmic}
\end{algorithm}

\end{document}
